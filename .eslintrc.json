{
    "env": {
        "browser": true,
        "es2020": true
    },
    "extends": [
        "airbnb"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "no-console": 0,
        "no-shadow": 0,
        "import/extensions": 0,
        "import/no-unresolved": 0,
        "no-unused-vars": 1,
        "max-len": ["error", { "code": 140 }],
        "max-classes-per-file": 0,
        "no-use-before-define": 0,
        "lines-between-class-members": 0,
        "no-loop-func": 0
    }
}
